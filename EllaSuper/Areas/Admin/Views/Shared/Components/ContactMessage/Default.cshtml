@using Ella.Core.Entity
@model List<ContactMessage>
@{
    var messages = Model;
}

<li class="item">
    <img src="data:image/png;base64," /> 
    <ul>
        @foreach (var item in messages)
        {
            <li>
                <h5>@item.Email</h5>
               <span>
                    @item.Message
               </span>
            </li>
        }
    </ul>
</li>